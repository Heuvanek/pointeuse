{% extends 'base.html.twig' %}
{% block title %}{{ parent() }} - Ajouter Client{% endblock %}
{% block body %}
  <form class="d-flex justify-content-center">
  <fieldset>
    <div class="form-group">
        <label class="col-form-label" for="tempsT">Société</label>
        <input type="text" class="form-control" placeholder="Société T" id="societe">
      </div>
    <div class="form-group">
        <label class="col-form-label" for="tempsT">Nom</label>
        <input type="text" class="form-control" placeholder="Nom" id="nom">
      </div>
    <div class="form-group">
        <label class="col-form-label" for="tempsT">Prénom</label>
        <input type="text" class="form-control" placeholder="Prénom" id="prenom">
      </div>
    <div class="form-group">
        <label class="col-form-label" for="tempsT">Téléphone</label>
        <input type="text" class="form-control" placeholder="Téléphone" id="tel">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Adresse email</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
      </div>
      <div>
          <button type="button" class="btn btn-info btn-lg btn-block">Ajouter</button>
      </div>
  
  </fieldset>
</form>
{% endblock %}